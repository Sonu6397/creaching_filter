@model ManualReadingopration.dbo_connt.UserLogin<div class="container">    <div class="row mx-5">        <div class="col-md-4  col-md-offset-4">            <div class="login-panel panel panel-default">                <div class="panel-heading">                    <h3 class="fw-bold mb-5 text-center">Login</h3>                    <div class="alert-danger">                        @if (TempData["LoginId"] != null)                        {                        @TempData["LoginId"]}                    </div>                </div>                <div class="panel-body">                    @using (Html.BeginForm("Login", "Home", FormMethod.Post))                    {        <form role="form">            <fieldset>                <div class="form-group">                    @Html.TextBoxFor(model => model.Engineer_Name, new { @class = "form-control",@placeholder="Engineer_Name" })                    @Html.ValidationMessageFor(model => model.Engineer_Name, "", new { @class = "error" })                </div>                <div class="form-group">                    @Html.TextBoxFor(model => model.Password, new { @class = "form-control",@placeholder="Password" })                    @Html.ValidationMessageFor(model => model.Password, "", new { @class = "error" })                </div>                <div class="form-group">                    <button type="submit" class="btn btn-primary btn-lg btn-block">Continue</button>                </div>            </fieldset>        </form>}                </div>            </div>        </div>    </div></div><style>    .panel-default {        border-color: #ddd;        margin-top: 10rem;    }    .form-control {        display: block;        width: 100%;        height: 34px;        margin-left: 22px;        padding: 6px 12px;        font-size: 14px;        line-height: 1.42857143;        color: #555555;        background-color: #fff;        background-image: none;        border: 1px solid #ccc;        border-radius: 4px;        -webkit-box-shadow: inset 0 1px 1px rgb(0 0 0 / 8%);        box-shadow: inset 0 1px 1px rgb(0 0 0 / 8%);        -webkit-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;        -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;        -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;        transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;        transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;        transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;    }    .btn-lg, .btn-group-lg > .btn {        padding: 2px 16px;        font-size: 18px;        line-height: 1.3333333;        border-radius: 6px;    }</style>@section Scripts {    @Scripts.Render("~/bundles/jqueryval")}